<Window x:Class="BreakBlock.MainWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:testbk"
		mc:Ignorable="d"
		Title="BreakBlock" Width="656" Height="518"
		ResizeMode="NoResize"
		PreviewKeyDown="keyDown"
		PreviewKeyUp="keyUp">
	<Canvas Name="Field" Width="640" Height="480" Background="#000099"></Canvas>
	<x:Code><![CDATA[
		//キーイベント
		void keyDown(object sender,KeyEventArgs e){BBUtil.CursorKey.keyDown(e);}
		void keyUp(object sender,KeyEventArgs e){BBUtil.CursorKey.keyUp(e);}
	]]></x:Code>
</Window>
